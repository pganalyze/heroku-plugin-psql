{
  "name": "heroku-plugin-psql",
  "description": "Like heroku pg:psql but for any Postgres database",
  "version": "0.0.7",
  "author": "Maciek Sakrejda @uhoh-itsmaciek",
  "bugs": "https://github.com/pganalyze/heroku-plugin-psql/issues",
  "dependencies": {
    "@heroku-cli/color": "^1.1.14",
    "@heroku-cli/command": "^8.3.0",
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@types/debug": "^4.1.5",
    "@types/supports-color": "^5.3.0",
    "debug": "^4.1.1",
    "tslib": "^1"
  },
  "devDependencies": {
    "@heroku-cli/schema": "^1.0.25",
    "@oclif/dev-cli": "^1",
    "@oclif/plugin-help": "^3",
    "@oclif/test": "^1",
    "@types/chai": "^4",
    "@types/mocha": "^5",
    "@types/nock": "^11.1.0",
    "@types/node": "^10",
    "chai": "^4",
    "eslint": "^5.13",
    "eslint-config-oclif": "^3.1",
    "eslint-config-oclif-typescript": "^0.1",
    "globby": "^10",
    "mocha": "^5",
    "nyc": "^14",
    "ts-node": "^8",
    "typescript": "^3.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/yarn.lock"
  ],
  "homepage": "https://github.com/pganalyze/heroku-plugin-psql",
  "keywords": [
    "oclif-plugin"
  ],
  "license": "MIT",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "run",
    "devPlugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "pganalyze/heroku-plugin-psql",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "tidy": "eslint . --ext .ts --config .eslintrc --fix",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"src/**/*.test.ts\"",
    "version": "true"
  }
}
